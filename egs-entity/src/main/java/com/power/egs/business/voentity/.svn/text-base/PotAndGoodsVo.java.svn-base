package com.power.egs.business.voentity;

import java.io.Serializable;


public class PotAndGoodsVo implements Serializable{
	private static final long serialVersionUID = 1L;
	private String potNo;
	private String goodsCode;
	private String goodsName;
	private String writeAmount;  //该字段用于冲销的核销量   和入库的入库量
	
	public String costPrice;//成本价  该字段用于入库保存时
	
	public String getWriteAmount() {
		return writeAmount;
	}
	public void setWriteAmount(String writeAmount) {
		this.writeAmount = writeAmount;
	}
	public String getPotNo() {
		return potNo;
	}
	public String getCostPrice() {
		return costPrice;
	}
	public void setCostPrice(String costPrice) {
		this.costPrice = costPrice;
	}
	public void setPotNo(String potNo) {
		this.potNo = potNo;
	}
	public String getGoodsCode() {
		return goodsCode;
	}
	public void setGoodsCode(String goodsCode) {
		this.goodsCode = goodsCode;
	}
	public String getGoodsName() {
		return goodsName;
	}
	public void setGoodsName(String goodsName) {
		this.goodsName = goodsName;
	}
	
	
}
